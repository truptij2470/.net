@model IEnumerable<WebMvc1.Models.Department>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
@{

    Object msg = TempData.TryGetValue("msg",out msg);
    String msg1 = Convert.ToString(msg);
    <h4>@msg1</h4>
}
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeptNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeptName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeptNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeptName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.DeptNo }) |
                @Html.ActionLink("Details", "Details", new {  id=item.DeptNo }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.DeptNo })
            </td>
        </tr>
}
    </tbody>
</table>
